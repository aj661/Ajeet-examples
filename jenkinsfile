pipeline {
    agent any
        stages { 
            stage('stage 1'){
                steps{
                    echo "hellow"
                }
            }
            stage('stage 2'){
                steps{
                    sh 'apache2 --version'
               }
            }
            stage('stage 3'){
                steps {
                    sh 'ansible --version'
                }
            }
            stage('stage 4'){
                steps {
                    sh 'git --version'
                }
            }
            
        }   
}
